{
  "name": "@dcloudio/unicloud-server-plugin",
  "version": "1.0.1",
  "description": "server plugin for uniCloud",
  "main": "dist/index.js",
  "homepage": "https://github.com/dcloudio/uniCloud-server-plugin.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcloudio/uniCloud-server-plugin.git"
  },
  "scripts": {
    "lint": "eslint --fix src build",
    "build": "npm run lint && rollup -c build/rollup.config.js --environment NODE_ENV:production",
    "dev": "rollup -w -c build/rollup.config.js --environment NODE_ENV:development",
    "build:all": "npm run lint && rollup -c build/rollup.config.all.js --environment NODE_ENV:production",
    "dev:all": "rollup -w -c build/rollup.config.all.js --environment NODE_ENV:development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "rollup": "^1.28.0",
    "rollup-plugin-terser": "^5.1.3"
  }
}
